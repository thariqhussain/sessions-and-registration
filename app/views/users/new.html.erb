<h1>Sign Up</h1>

<%= form_with model: @user do |form| %>
<div>
    <%= form.label:email %>
    <%= form.email_field:email %>
</div>

<div>
    <%= form.label:password %>
    <%= form.password_field:password %>
</div>

<div><%= form.submit "Sign up" %></div>
<% end %>

<%= link_to "login", @login_path %>